			type="numeric" minLength="0" maxLength="3" defaultValue="20" />

		<choiceField id="PreAuthClearPolicy" name="PreAuth Clear Policy during Settlement" type="SINGLE"
			defaultValue="0" access="READWRITE">
			<choiceValue label="No clearing without prompt" value="0" />
			<choiceValue label="Clearing without prompt" value="1" />
			<choiceValue label="Prompt and clear if requested" value="2" />
		</choiceField>

		<dataField id="PreAuthMaxAge" name="PreAuth max age (days)" access="READWRITE"
			type="numeric" minLength="0" maxLength="3" defaultValue="30" />

		<dataField id="PreAuthCompletionAllowedVariance" name="PreAuth Completion Variance (%)" access="READWRITE"
			type="numeric" minLength="0" maxLength="3" defaultValue="0" />

		<dataField id="SupervisorPassword" name="Supervisor Password" access="READWRITE"
			type="alphanumeric" minLength="0" maxLength="100" defaultValue="" />
		<dataField id="ManagerPassword" name="Manager Password" access="READWRITE"
			type="alphanumeric" minLength="0" maxLength="100" defaultValue="" />

		<choiceField id="VoidPasswordLevel" name="Void Password Level" type="SINGLE"
			defaultValue="2" access="READWRITE">
			<choiceValue label="Manager" value="1" />
			<choiceValue label="Supervisor" value="2" />
			<choiceValue label="Access Freely" value="3" />
		</choiceField>
		<choiceField id="RefundPasswordLevel" name="Refund Password Level" type="SINGLE"
			defaultValue="1" access="READWRITE">
			<choiceValue label="Manager" value="1" />
			<choiceValue label="Supervisor" value="2" />
			<choiceValue label="Access Freely" value="3" />
		</choiceField>
		<choiceField id="OfflinePasswordLevel" name="Offline Password Level" type="SINGLE"
			defaultValue="2" access="READWRITE">
			<choiceValue label="Manager" value="1" />
			<choiceValue label="Supervisor" value="2" />
			<choiceValue label="Access Freely" value="3" />
		</choiceField>
		<choiceField id="SettlementPasswordLevel" name="Settlement Password Level" type="SINGLE"
			defaultValue="2" access="READWRITE">
			<choiceValue label="Manager" value="1" />
			<choiceValue label="Supervisor" value="2" />
			<choiceValue label="Access Freely" value="3" />
		</choiceField>
		
		<choiceField id="TleEnabled" name="Terminal Line Encryption" type="SINGLE"
			defaultValue="0" access="READWRITE">
			<choiceValue label="Disabled" value="0" />
			<choiceValue label="Enabled" value="1" />
		</choiceField>

		<choiceField id="PiggybackEnabled" name="Piggyback Advices" type="SINGLE"

			</choiceField>
			<choiceField id="ManualEntryEnabled" name="Manual Entry" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			
			<choiceField id="CardDccEnabled" name="DCC" type="SINGLE"
				defaultValue="0" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />

		<choiceField id="EmvEnabled" name="EMV" type="SINGLE" defaultValue="1" access="READWRITE">
			<choiceValue label="Disabled" value="0" />
			<choiceValue label="Enabled" value="1" />
		</choiceField>
		<choiceField id="InstalmentDataToHostEnabled" name="Instalment Data To Host" type="SINGLE"
			defaultValue="1" access="READWRITE">
			<choiceValue label="Disabled" value="0" />
			<choiceValue label="Enabled" value="1" />
		</choiceField>

		<dataField id="TerminalIdMasking" name="Terminal Id Mask" access="READWRITE"
			type="alphanumeric" minLength="0" maxLength="8" defaultValue="" />
		<dataField id="MerchantIdMasking" name="Merchant Id Mask" access="READWRITE"
			type="alphanumeric" minLength="0" maxLength="15" defaultValue="" />

		<dataField id="RefundMaxAmount" name="Refund Max Amount" access="READWRITE"
			type="numeric" minLength="0" maxLength="12" defaultValue="" />
		<dataField id="RefundPasswordFloorLimit" name="Refund Password Floor Limit" access="READWRITE"
			type="numeric" minLength="0" maxLength="12" defaultValue="" />
		<choiceField id="RefundClessEnabled" name="Contactless Refund" type="SINGLE"
			defaultValue="1" access="READWRITE">
			<choiceValue label="Disabled" value="0" />
			<choiceValue label="Enabled" value="1" />
		</choiceField>
		
		<choiceField id="DccEnabled" name="DCC" type="SINGLE"
			defaultValue="0" access="READWRITE">
			<choiceValue label="Disabled" value="0" />
			<choiceValue label="Enabled" value="1" />
		</choiceField>
		
		<choiceField id="DccProviderOption" name="DCC Provider" type="SINGLE"
			defaultValue="0" access="READWRITE">
			<choiceValue label="Fexco" value="0" />
			<choiceValue label="Pure Commerce" value="1" />
			<choiceValue label="Fintrax" value="2" />
		</choiceField>
		
		<choiceField id="DccStaffIdInputEnabled" name="DCC Staff Id Input" type="SINGLE"
			defaultValue="1" access="READWRITE">
			<choiceValue label="Disabled" value="0" />
			<choiceValue label="Enabled" value="1" />
		</choiceField>
		<dataField id="IdleMessage" name="Idle Message" access="READWRITE"
			type="alphanumeric" minLength="0" maxLength="200" defaultValue="" />

		<!-- HOST TABLE -->
		<structureField id="Hosts" name="Hosts" access="READWRITE"
			minRows="0" maxRows="300" uniqueKeys="HostIndex">

			<dataField id="HostIndex" name="Index" access="READWRITE"
				type="numeric" minLength="0" maxLength="3" defaultValue="" />
			<dataField id="HostName" name="Host Name" access="READWRITE" type="alphanumeric"
				minLength="1" maxLength="100" defaultValue="" />
			<choiceField id="HostProtocol" name="Host Protocol" type="SINGLE"
				defaultValue="0" access="READWRITE">
				<choiceValue label="FDMS BASE 24" value="0" />
				<choiceValue label="AMEX DIRECT" value="1" />
			</choiceField>
			<dataField id="CommsHostName" name="Comms Host Name" access="READWRITE" type="alphanumeric"
				minLength="1" maxLength="100" defaultValue="" />

			<dataField id="TerminalId" name="Terminal ID" access="READWRITE" type="alphanumeric"
				minLength="1" maxLength="8" defaultValue="" />
			<dataField id="MerchantId" name="Merchant ID" access="READWRITE" type="alphanumeric"
				minLength="1" maxLength="15" defaultValue="" />

			<dataField id="SettlementTpdu" name="Settlement TPDU (hex)" access="READWRITE" type="hexadecimal"
				minLength="0" maxLength="100" defaultValue="6000310000" />
			<dataField id="SettlementNii" name="Settlement Nii" access="READWRITE" type="hexadecimal"
				minLength="0" maxLength="4" defaultValue="031" />

			<choiceField id="BatchUploadEnabled" name="Batch Upload" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>

			<dataField id="HostTimeout" name="Global Timeout (ms)" access="READWRITE" type="numeric"
				minLength="1" maxLength="20" defaultValue="30000" />
			<dataField id="PreDefinedAuthCode" name="Auth Code for Offline Sale" access="READWRITE" type="alphanumeric"
				minLength="0" maxLength="6" defaultValue="" />
			<choiceField id="PredialEnabled" name="Predial" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
		</structureField>

		<!-- ISSUER TABLE -->
		<structureField id="Issuers" name="Issuers" access="READWRITE"

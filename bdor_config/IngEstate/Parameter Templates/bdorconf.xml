			<dataField id="IssuerIndex" name="Index" access="READWRITE"
				type="numeric" minLength="0" maxLength="3" defaultValue="" />
			<dataField id="IssuerName" name="Issuer Name" access="READWRITE" type="alphanumeric"
				minLength="1" maxLength="100" defaultValue="" />
				
			<dataField id="Tpdu" name="TPDU (hex)" access="READWRITE" type="hexadecimal"
				minLength="0" maxLength="100" defaultValue="" />
			<dataField id="Nii" name="Nii" access="READWRITE" type="hexadecimal"
				minLength="0" maxLength="4" defaultValue="" />

			<dataField id="PanMaskDisplay" name="Pan Mask for Display" access="READWRITE"
				type="alphanumeric" minLength="0" maxLength="30" defaultValue="" />
			<dataField id="PanMaskMerchantReceipt" name="Pan Mask for Merchant Receipt" access="READWRITE"
				type="alphanumeric" minLength="0" maxLength="30" defaultValue="" />
			<dataField id="PanMaskCustomerReceipt" name="Pan Mask for Customer Receipt" access="READWRITE"

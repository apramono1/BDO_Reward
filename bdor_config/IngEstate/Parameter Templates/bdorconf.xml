	<layout>
		<container name="Terminal" type="TAB">
			<fieldLayout fieldId="TerminalBaseCurrencyCode" type="TEXT" />
			<fieldLayout fieldId="MinimumAmount" type="TEXT" />
			<fieldLayout fieldId="MaximumAmount" type="TEXT" />
			<fieldLayout fieldId="OfflineSaleMaxAmount" type="TEXT" />
			<fieldLayout fieldId="SaleContactlessEnabled" type="RADIOBUTTON" />

			<separatorLayout text="Receipts settings" />
			<fieldLayout fieldId="ReceiptHeaderLine1" type="TEXT" />
			<fieldLayout fieldId="ReceiptHeaderLine2" type="TEXT" />
			<fieldLayout fieldId="ReceiptHeaderLine3" type="TEXT" />

			<dataField id="ExpiryDateMaskCustomerReceipt" name="Expiry Date for Customer Receipt" access="READWRITE"
				type="alphanumeric" minLength="0" maxLength="30" defaultValue="" />
		</structureField>

		<!-- CARD TABLE -->
		<structureField id="Cards" name="Cards" access="READWRITE"
			minRows="0" maxRows="999" uniqueKeys="CardIndex">

			<dataField id="CardIndex" name="Index" access="READWRITE"
				type="numeric" minLength="0" maxLength="3" defaultValue="" />

			<dataField id="PanLowRange" name="Pan Low Range" access="READWRITE"
				type="numeric" minLength="0" maxLength="20" defaultValue="" />
			<dataField id="PanHighRange" name="Pan High Range" access="READWRITE"
				type="numeric" minLength="0" maxLength="20" defaultValue="" />
			<dataField id="PanMinLength" name="Pan Min Length" access="READWRITE"
				type="numeric" minLength="1" maxLength="2" defaultValue="1" />
			<dataField id="PanMaxLength" name="Pan Max Length" access="READWRITE"
				type="numeric" minLength="1" maxLength="2" defaultValue="19" />

			<dynamicChoiceField id="CardHostIndex" name="Host Index" access="READWRITE"
				refFieldId="Hosts" refLabel="HostName" refValue="HostIndex"/>
			<dynamicChoiceField id="CardMotoHostIndex" name="MOTO Host Index" access="READWRITE"
				refFieldId="Hosts" refLabel="HostName" refValue="HostIndex"/>
			<dynamicChoiceField id="CardIssuerIndex" name="Issuer" access="READWRITE"
				refFieldId="Issuers" refLabel="IssuerName" refValue="IssuerIndex"/>

			<dataField id="FloorLimit" name="Floor Limit" access="READWRITE"
				type="numeric" minLength="0" maxLength="12" defaultValue="" />
			<dataField id="MotoFloorLimit" name="MOTO Floor Limit" access="READWRITE"
				type="numeric" minLength="0" maxLength="12" defaultValue="" />
			<dataField id="QpsFloorLimit" name="QPS Floor Limit" access="READWRITE"
				type="numeric" minLength="0" maxLength="12" defaultValue="" />

			<choiceField id="CvvManualEntry" name="CVV for Manual Entry" type="SINGLE"
				defaultValue="0" access="READWRITE">
				<choiceValue label="No CVV prompt" value="0" />
				<choiceValue label="CVV prompt and allow bypass" value="1" />
				<choiceValue label="CVV prompt and no bypass" value="2" />
			</choiceField>
			<choiceField id="CvvMotoSale" name="CVV for MOTO Sale" type="SINGLE"
				defaultValue="0" access="READWRITE">
				<choiceValue label="No CVV prompt" value="0" />
				<choiceValue label="CVV prompt and allow bypass" value="1" />
				<choiceValue label="CVV prompt and no bypass" value="2" />
			</choiceField>
			<choiceField id="CvvMagstripeEntry" name="CVV for Magstripe Entry" type="SINGLE"
				defaultValue="0" access="READWRITE">
				<choiceValue label="No CVV prompt" value="0" />
				<choiceValue label="CVV prompt and allow bypass" value="1" />
				<choiceValue label="CVV prompt and no bypass" value="2" />
			</choiceField>

			<choiceField id="CardSaleEnabled" name="Sale" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardPreAuthEnabled" name="PreAuth" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardOfflineSaleEnabled" name="Offline Sale" type="SINGLE"

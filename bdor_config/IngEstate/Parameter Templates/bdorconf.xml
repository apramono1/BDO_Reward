			</choiceField>
			<choiceField id="CardRefundEnabled" name="Refund" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardVoidEnabled" name="Void" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardVoidPreAuthEnabled" name="Void Preauth" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="PreAuthCompOnlineEnabled" name="Preauth-Comp Online" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="PreAuthCompOfflineEnabled" name="Preauth-Comp Offline" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardMotoSaleEnabled" name="MOTO Sale" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardMotoRefundEnabled" name="MOTO Refund" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardMotoPreauthEnabled" name="MOTO Preauth" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
			</choiceField>
			<choiceField id="CardInstalmentSaleEnabled" name="Instalment Sale" type="SINGLE"
				defaultValue="1" access="READWRITE">
				<choiceValue label="Disabled" value="0" />
				<choiceValue label="Enabled" value="1" />
